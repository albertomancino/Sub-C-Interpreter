all: compiler
	clear

compiler: lex.yy.c debuggingparser.tab.c debuggingparser.tab.h
	gcc lex.yy.c debuggingparser.tab.c -ll -lm main.c

lex.yy.c: debugginglexer.l debuggingparser.tab.h
	flex debugginglexer.l

debuggingparser.tab.c debuggingparser.tab.h: debuggingparser.y
	bison -d debuggingparser.y
